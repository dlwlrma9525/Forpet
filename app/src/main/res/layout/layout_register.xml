<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <ImageView
        android:id="@+id/logo_register"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_hospital" />

    <TextView
        android:id="@+id/header_register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="14dp"
        android:fontFamily="@font/nanum_square"
        android:includeFontPadding="false"
        android:text="@string/register_title"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/logo_register"
        app:layout_constraintStart_toEndOf="@+id/logo_register"
        app:layout_constraintTop_toTopOf="@+id/logo_register" />

    <TextView
        android:id="@+id/text_notice"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="17dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="17dp"
        android:background="@drawable/background_stroke_corners_8dp"
        android:fontFamily="@font/nanum_square"
        android:includeFontPadding="false"
        android:padding="8dp"
        android:text="@string/register_notice"
        android:textColor="#ff6586"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo_register" />

    <LinearLayout
        android:id="@+id/linear_layout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="22dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_notice">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/register_name"
                android:textColor="#000000"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/edit_place_name"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_stroke_corners_8dp"
                android:fontFamily="@font/nanum_square"
                android:inputType="text"
                android:textColor="#000000"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/business_no"
                android:textColor="#000000"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/edit_business_no"
                android:layout_width="120dp"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_stroke_corners_8dp"
                android:fontFamily="@font/nanum_square"
                android:inputType="text"
                android:textColor="#000000"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/ceo_name"
                android:textColor="#000000"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/edit_ceo_name"
                android:layout_width="120dp"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_stroke_corners_8dp"
                android:fontFamily="@font/nanum_square"
                android:inputType="text"
                android:textColor="#000000"
                android:textSize="15sp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="6dp"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/ceo_name_notice"
                android:textColor="#000000"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/address_name"
                android:textColor="#000000"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/edit_address_name"
                android:layout_width="120dp"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_stroke_corners_8dp"
                android:fontFamily="@font/nanum_square"
                android:inputType="text"
                android:textColor="#000000"
                android:textSize="15sp" />

            <Button
                android:id="@+id/button_address"
                android:layout_width="69dp"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_corners_pink_8dp"
                android:text="@string/select"
                android:textColor="#ffffff"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/address_name_detail"
                android:textColor="#a7a7a7"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/edit_address_name_detail"
                android:layout_width="120dp"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_stroke_corners_8dp"
                android:fontFamily="@font/nanum_square"
                android:inputType="text"
                android:textColor="#000000"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/charge_name"
                android:textColor="#000000"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/edit_charge_name"
                android:layout_width="120dp"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_stroke_corners_8dp"
                android:fontFamily="@font/nanum_square"
                android:inputType="text"
                android:textColor="#000000"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="6dp">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/phone"
                android:textColor="#000000"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/edit_phone"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_stroke_corners_8dp"
                android:fontFamily="@font/nanum_square"
                android:inputType="phone"
                android:textColor="#000000"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/email"
                android:textColor="#000000"
                android:textSize="15sp" />

            <EditText
                android:id="@+id/edit_email"
                android:layout_width="match_parent"
                android:layout_height="34dp"
                android:layout_marginStart="6dp"
                android:background="@drawable/background_stroke_corners_8dp"
                android:fontFamily="@font/nanum_square"
                android:inputType="text|textEmailAddress"
                android:textColor="#000000"
                android:textSize="15sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:layout_width="69dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/nanum_square"
                android:includeFontPadding="false"
                android:text="@string/opt"
                android:textColor="#000000"
                android:textSize="15sp" />

            <GridLayout
                android:id="@+id/grid_layout_opt"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="6dp" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>